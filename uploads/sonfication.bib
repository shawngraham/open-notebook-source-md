	Language = {English},
	Number = {1},
	Pages = {pp. 81-103},
	Publisher = {The University of Chicago Press on behalf of Wenner-Gren Foundation for Anthropological Research},
	Title = {The Acoustic and Auditory Contexts of Human Behavior},
	Url = {http://www.jstor.org/stable/10.1086/679445},
	Volume = {56},
